{
  "name": "slack-mocker",
  "version": "2.0.1",
  "description": "Slack API mocker",
  "main": "build/index.js",
  "typings": "index",
  "scripts": {
    "build": "tsc",
    "test": "istanbul cover --report lcov --dir ./coverage/ _mocha --recursive ./test/ --grep ./test/**/*.spec.js -- --colors --reporter spec",
    "ci": "npm run build && npm test && cat ./coverage/lcov.info | coveralls"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skellington-Closet/slack-mocker.git"
  },
  "dependencies": {
    "express": "^4.14.0",
    "nock": "^10.0.6",
    "qs": "^6.3.0",
    "request": "^2.79.0",
    "winston": "^2.3.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "@types/nock": "^10.0.1",
    "@types/node": "10.14.4",
    "@types/qs": "^6.5.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "*",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "delay": "^1.3.1",
    "husky": "1.3.1",
    "istanbul": "^0.4.5",
    "lint-staged": "8.1.5",
    "mocha": "^3.2.0",
    "prettier": "1.16.4",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "typescript": "^3.4.5"
  },
  "lint-staged": {
    "src/**/**.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
