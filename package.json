{
  "name": "slack-mocker",
  "version": "2.0.2",
  "description": "Slack API mocker written in TypeScript",
  "main": "build/index.js",
  "typings": "build/index",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "ci": "npm run build && npm test && cat coverage/lcov.info | coveralls"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skellington-Closet/slack-mocker.git"
  },
  "dependencies": {
    "qs": "^6.3.0",
    "winston": "^2.3.0",
    "nock": "^10.0.6"
  },
  "devDependencies": {
    "request": "^2.79.0",
    "@types/nock": "^10.0.1",
    "ts-jest": "^24.0.2",
    "@types/request": "^2.48.1",
    "@types/proxyquire": "^1.3.28",
    "jest": "^24.8.0",
    "@types/jest": "^24.0.12",
    "@types/node": "10.14.4",
    "@types/qs": "^6.5.3",
    "@types/sinon": "^7.0.11",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.15.2",
    "coveralls": "^2.11.15",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "typescript": "^3.4.5"
  },
  "lint-staged": {
    "src/**/**.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
